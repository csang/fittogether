	<div class="post challenges">
		<div class="profile_picture">
			<div class="user"><%= image_tag('profile/trainer.png') %></div>
		</div>
		<div class="top orange">
			<h3 class='orange'>ANDREW LUCK</h3>
			<h4>1 HOUR AGO</h4>
			<h2>CHALLENGE</h2>
			<span><%= image_tag('icons/orange_arrow.png') %></span>
		</div>
		<div class="data">
			<div class="info">
				<div class="time">
					<h3>TIME</h3>
					<h4>DAY</h4>
					<h4>HRS</h4>
					<h4>MIN</h4>
					<h2><span class="days">1</span>:<span class="hours">11</span>:<span class="mins">59</span></h2>
				</div>
				<div class="bet">
					<h3>BET</h3>
					<h2>120</h2>
					<%= image_tag('global/fp.png') %>
				</div>
			</div>
			<div class="standing">
				<div class="winning">
					<div class="person">
						<h3>WINNING</h3>
						<div class="avatar"><%= image_tag('profile/trainer.png') %></div>
						<h3>ANDREW LUCK</h3>
					</div>
					<div class="score">
						<h2><span>12,458</span> STEPS</h2>
					</div>
				</div>
				<div class="challenge">
					<div class="badge">
						<div class="type">
							<h4>CHALLENGE</h4>
							<%= image_tag('activities/running.png') %>
						</div>
						<div class="goal">
							<h3>18K STEP</h3>
						</div>	
					</div>
				</div>
				<div class="vs">
					<%= image_tag('global/vs.png') %>
				</div>
				<div class="losing">
					<div class="person">
						<h3>LOSING</h3>
						<div class="avatar"><%#= image_tag(@account.account_user.avatar) %></div>
						<h3>CARLOS SANG</h3>
					</div>
					<div class="score">
						<h2><span>11,008</span> STEPS</h2>
					</div>
				</div>
			</div>
		</div>
		<div class="comment_box">
			<textarea placeholder="Type comment..." style="resize: none"></textarea>
		</div>
		<div class="bottom">
			<div class="synch_fitbit">
				<%= image_tag('fitbit/synchs_with_fitbit_b.png') %>
			</div>
			<%= link_to('Give Kudos!', '/') %>
			<%= link_to('Comment', '/', html_options={:class => 'add_comment'}) %>
		</div>
	</div>

