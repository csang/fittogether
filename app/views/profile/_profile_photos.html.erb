<div class="profile_photos_view">
	<div class="photos_sub_nav">
		<div class="breadcrumbs">
			<p><span>Album</span>><span>Big Life Moments</span></p>
		</div>
		<div class="photo_options">
			<div class="amount">
				<p>8 Photos</p>
			</div>
		</div>
	</div>
	<div class="collection">
		<% ['1','2','3','4','5','6','7','8'].each do |image| %>
		<div class="photo">
			<div class="on_hover">
				<%= image_tag('icons/photos_view.png') %>
				<%= image_tag('icons/photos_comment.png') %>
				<span>28</span>
			</div>
			<%= image_tag('photos/'+image+'.png') %>
		</div>
		<% end %>
	</div>
</div>